{
  "name": "apod",
  "version": "0.0.1",
  "author": "Ralph Schaer",
  "homepage": "https://github.com/ralscha/apod",
  "private": true,
  "scripts": {
    "clean": "ionic-app-scripts clean",
    "build": "ionic-app-scripts build",
    "lint": "ionic-app-scripts lint",
    "ionic:build": "ionic-app-scripts build",
    "ionic:serve": "ionic-app-scripts serve",
    "pbts": "pbjs -t static-module ../server/src/main/protobuf/Apod.proto -o src/protos/apod.js && pbts --no-comments src/protos/apod.js -o src/protos/apod.d.ts",
    "dist": "shx rm -rf www && npm run build --prod && node ./cache-busting.js && workbox injectManifest && npm run minify-html && npm run compress",
    "compress": "bread-compressor -a gzip www",
    "minify-html": "html-minifier -o www/index.html www/index.html --collapse-whitespace --remove-comments",
    "open": "http-server www -o -a localhost -p 1234"
  },
  "config": {
    "ionic_copy": "./copy.config.js",
    "ionic_webpack": "./webpack.config.js"
  },
  "dependencies": {
    "@angular/common": "5.2.9",
    "@angular/compiler": "5.2.9",
    "@angular/compiler-cli": "5.2.9",
    "@angular/core": "5.2.9",
    "@angular/forms": "5.2.9",
    "@angular/http": "5.2.9",
    "@angular/platform-browser": "5.2.9",
    "@angular/platform-browser-dynamic": "5.2.9",
    "dexie": "2.0.2",
    "ionic-angular": "3.9.2",
    "ionicons": "3.0.0",
    "protobufjs": "6.8.6",
    "rxjs": "5.5.8",
    "workbox-sw": "3.0.1",
    "zone.js": "0.8.20"
  },
  "devDependencies": {
    "@ionic/app-scripts": "3.1.8",
    "bread-compressor-cli": "1.0.0",
    "html-minifier": "3.5.12",
    "http-server": "0.11.1",
    "rev-hash": "2.0.0",
    "shx": "0.2.2",
    "typescript": "2.7.2",
    "webpack-manifest-plugin": "1.3.2",
    "workbox-cli": "3.0.1"
  },
  "description": "An apod client with Ionic"
}
