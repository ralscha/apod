{
  "name": "apod",
  "version": "0.0.1",
  "scripts": {
    "ng": "ng",
    "dev": "npm-run-all --parallel sw-dev-webpack ionic-start",
    "start": "ng serve -o",
    "ionic-start": "ionic serve",
    "build": "ng build",
    "postbuild": "npm run sw-prod-webpack && bread-compressor dist/app",
    "serve-dist": "ws --hostname localhost -d dist/app -p 1234 -o --log.format stats",
    "lint": "ng lint",
    "pbts": "pbjs -t static-module ../server/src/main/protobuf/Apod.proto -o src/app/protos/apod.js && pbts --no-comments src/app/protos/apod.js -o src/app/protos/apod.d.ts",
    "sw-dev-webpack": "rimraf ./src/service-worker.js && webpack --config ./sw-dev-webpack.config.js",
    "sw-prod-webpack": "rimraf ./dist/app/service-worker.js && webpack --config ./sw-prod-webpack.config.js --progress --color && workbox injectManifest ./workbox-config.js"
  },
  "private": true,
  "dependencies": {
    "@angular/common": "13.2.6",
    "@angular/compiler": "13.2.6",
    "@angular/core": "13.2.6",
    "@angular/forms": "13.2.6",
    "@angular/platform-browser": "13.2.6",
    "@angular/platform-browser-dynamic": "13.2.6",
    "@angular/router": "13.2.6",
    "@ionic/angular": "6.0.11",
    "dexie": "3.2.1",
    "protobufjs": "6.11.2",
    "pwacompat": "2.0.17",
    "rxjs": "7.5.5",
    "tslib": "2.3.1",
    "workbox-cacheable-response": "6.5.1",
    "workbox-core": "6.5.1",
    "workbox-expiration": "6.5.1",
    "workbox-precaching": "6.5.1",
    "workbox-routing": "6.5.1",
    "workbox-strategies": "6.5.1",
    "workbox-window": "6.5.1",
    "zone.js": "0.11.5"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "13.2.6",
    "@angular-eslint/builder": "13.1.0",
    "@angular-eslint/eslint-plugin": "13.1.0",
    "@angular-eslint/eslint-plugin-template": "13.1.0",
    "@angular-eslint/schematics": "13.1.0",
    "@angular-eslint/template-parser": "13.1.0",
    "@angular/cli": "13.2.6",
    "@angular/compiler-cli": "13.2.6",
    "@ionic/angular-toolkit": "6.1.0",
    "@types/node": "17.0.21",
    "@typescript-eslint/eslint-plugin": "5.14.0",
    "@typescript-eslint/parser": "5.14.0",
    "bread-compressor-cli": "1.1.0",
    "eslint": "8.10.0",
    "local-web-server": "5.2.0",
    "npm-run-all": "4.1.5",
    "rimraf": "3.0.2",
    "ts-loader": "9.2.8",
    "typescript": "4.5.5",
    "webpack-cli": "4.9.2",
    "workbox-cli": "6.5.1"
  }
}
