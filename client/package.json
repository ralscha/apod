{
  "name": "apod",
  "version": "0.0.1",
  "scripts": {
    "ng": "ng",
    "dev": "npm-run-all --parallel sw-dev-webpack ionic-start",
    "start": "ng serve -o",
    "ionic-start": "ionic serve",
    "build": "ng build",
    "postbuild": "npm run sw-prod-webpack && bread-compressor dist/app",
    "serve-dist": "ws --hostname localhost -d dist/app -p 1234 -o --log.format stats",
    "lint": "ng lint",
    "pbts": "pbjs -t static-module ../server/src/main/protobuf/Apod.proto -o src/app/protos/apod.js && pbts --no-comments src/app/protos/apod.js -o src/app/protos/apod.d.ts",
    "sw-dev-webpack": "rimraf ./src/service-worker.js && webpack --config ./sw-dev-webpack.config.js",
    "sw-prod-webpack": "rimraf ./dist/app/service-worker.js && webpack --config ./sw-prod-webpack.config.js --progress --color && workbox injectManifest ./workbox-config.js"
  },
  "private": true,
  "dependencies": {
    "@angular/common": "15.0.2",
    "@angular/compiler": "15.0.2",
    "@angular/core": "15.0.2",
    "@angular/forms": "15.0.2",
    "@angular/platform-browser": "15.0.2",
    "@angular/platform-browser-dynamic": "15.0.2",
    "@angular/router": "15.0.2",
    "@ionic/angular": "6.3.9",
    "dexie": "3.2.2",
    "protobufjs": "7.1.2",
    "pwacompat": "2.0.17",
    "rxjs": "7.5.7",
    "tslib": "2.4.1",
    "workbox-cacheable-response": "6.5.4",
    "workbox-core": "6.5.4",
    "workbox-expiration": "6.5.4",
    "workbox-precaching": "6.5.4",
    "workbox-routing": "6.5.4",
    "workbox-strategies": "6.5.4",
    "workbox-window": "6.5.4",
    "zone.js": "0.12.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "15.0.2",
    "@angular-eslint/builder": "15.1.0",
    "@angular-eslint/eslint-plugin": "15.1.0",
    "@angular-eslint/eslint-plugin-template": "15.1.0",
    "@angular-eslint/schematics": "15.1.0",
    "@angular-eslint/template-parser": "15.1.0",
    "@angular/cli": "15.0.2",
    "@angular/compiler-cli": "15.0.2",
    "@ionic/angular-toolkit": "7.0.0",
    "@typescript-eslint/eslint-plugin": "5.45.0",
    "@typescript-eslint/parser": "5.45.0",
    "bread-compressor-cli": "1.1.0",
    "eslint": "8.29.0",
    "local-web-server": "5.2.1",
    "npm-run-all": "4.1.5",
    "protobufjs-cli": "1.0.2",
    "rimraf": "3.0.2",
    "ts-loader": "9.4.2",
    "typescript": "4.8.4",
    "webpack-cli": "5.0.0",
    "workbox-cli": "6.5.4"
  }
}
