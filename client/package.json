{
  "name": "apod",
  "version": "0.0.1",
  "scripts": {
    "ng": "ng",
    "dev": "npm-run-all --parallel sw-dev-webpack ionic-start",
    "start": "ng serve -o",
    "ionic-start": "ionic serve",
    "build": "ng build",
    "build-prod": "ng build --prod",
    "postbuild-prod": "npm run sw-prod-webpack && bread-compressor dist/app",
    "serve-dist": "ws --hostname localhost -d dist/app -p 1234 -o --log.format stats",
    "lint": "ng lint",
    "pbts": "pbjs -t static-module ../server/src/main/protobuf/Apod.proto -o src/app/protos/apod.js && pbts --no-comments src/app/protos/apod.js -o src/app/protos/apod.d.ts",
    "sw-dev-webpack": "rimraf ./src/service-worker.js && webpack --config ./sw-dev-webpack.config.js",
    "sw-prod-webpack": "rimraf ./dist/app/service-worker.js && webpack --config ./sw-prod-webpack.config.js --progress --colors && workbox injectManifest ./workbox-config.js"
  },
  "private": true,
  "dependencies": {
    "@angular/common": "11.2.6",
    "@angular/compiler": "11.2.6",
    "@angular/core": "11.2.6",
    "@angular/forms": "11.2.6",
    "@angular/platform-browser": "11.2.6",
    "@angular/platform-browser-dynamic": "11.2.6",
    "@angular/router": "11.2.6",
    "@ionic/angular": "5.6.1",
    "dexie": "3.0.3",
    "protobufjs": "6.10.2",
    "pwacompat": "2.0.17",
    "rxjs": "6.6.7",
    "tslib": "2.1.0",
    "workbox-cacheable-response": "6.1.2",
    "workbox-core": "6.1.2",
    "workbox-expiration": "6.1.2",
    "workbox-precaching": "6.1.2",
    "workbox-routing": "6.1.2",
    "workbox-strategies": "6.1.2",
    "workbox-window": "6.1.2",
    "zone.js": "0.10.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "0.1102.5",
    "@angular/cli": "11.2.5",
    "@angular/compiler-cli": "11.2.6",
    "@ionic/angular-toolkit": "3.1.1",
    "@types/node": "14.14.35",
    "bread-compressor-cli": "1.1.0",
    "codelyzer": "6.0.1",
    "local-web-server": "4.2.1",
    "npm-run-all": "4.1.5",
    "rimraf": "3.0.2",
    "ts-loader": "8.0.18",
    "tslint": "6.1.3",
    "typescript": "4.1.5",
    "webpack": "4.44.2",
    "webpack-cli": "3.3.12",
    "workbox-cli": "6.1.2"
  }
}
