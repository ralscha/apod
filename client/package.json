{
  "name": "apod",
  "version": "0.0.1",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build-prod": "ng build --prod && workbox injectManifest && npm run compress",
    "serve-www": "http-server www -o -a localhost -p 1234",
    "lint": "ng lint",
    "pbts": "pbjs -t static-module ../server/src/main/protobuf/Apod.proto -o src/app/protos/apod.js && pbts --no-comments src/app/protos/apod.js -o src/app/protos/apod.d.ts",
    "compress": "bread-compressor www"
  },
  "private": true,
  "dependencies": {
    "@angular/common": "7.2.15",
    "@angular/core": "7.2.15",
    "@angular/forms": "7.2.15",
    "@angular/platform-browser": "7.2.15",
    "@angular/platform-browser-dynamic": "7.2.15",
    "@angular/router": "7.2.15",
    "@ionic/angular": "4.4.2",
    "core-js": "2.6.7",
    "dexie": "2.0.4",
    "protobufjs": "6.8.8",
    "rxjs": "6.5.2",
    "workbox-sw": "4.3.1",
    "workbox-window": "4.3.1",
    "zone.js": "0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/architect": "0.13.9",
    "@angular-devkit/build-angular": "0.13.9",
    "@angular-devkit/core": "7.3.9",
    "@angular-devkit/schematics": "7.3.9",
    "@angular/cli": "7.3.9",
    "@angular/compiler": "7.2.15",
    "@angular/compiler-cli": "7.2.15",
    "@angular/language-service": "7.2.15",
    "@ionic/angular-toolkit": "1.5.1",
    "@types/node": "11.13.12",
    "bread-compressor-cli": "1.0.6",
    "codelyzer": "4.5.0",
    "http-server": "0.11.1",
    "ts-node": "8.1.1",
    "tslint": "5.16.0",
    "typescript": "3.2.4",
    "workbox-cli": "4.3.1"
  }
}
