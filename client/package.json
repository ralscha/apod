{
  "name": "apod",
  "version": "0.0.1",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build-prod": "ng build --prod && workbox injectManifest",
    "serve-dist": "ws --hostname localhost -d dist/app -p 1234 -o --log.format stats",
    "lint": "ng lint",
    "pbts": "pbjs -t static-module ../server/src/main/protobuf/Apod.proto -o src/app/protos/apod.js && pbts --no-comments src/app/protos/apod.js -o src/app/protos/apod.d.ts",
    "postbuild-prod": "bread-compressor dist/app"
  },
  "private": true,
  "dependencies": {
    "@angular/common": "8.2.0",
    "@angular/compiler": "8.2.0",
    "@angular/core": "8.2.0",
    "@angular/forms": "8.2.0",
    "@angular/platform-browser": "8.2.0",
    "@angular/platform-browser-dynamic": "8.2.0",
    "@angular/router": "8.2.0",
    "@ionic/angular": "4.7.4",
    "dexie": "2.0.4",
    "protobufjs": "6.8.8",
    "rxjs": "6.5.2",
    "tslib": "1.10.0",
    "workbox-sw": "4.3.1",
    "workbox-window": "4.3.1",
    "zone.js": "0.10.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "0.802.0",
    "@angular/cli": "8.2.0",
    "@angular/compiler-cli": "8.2.0",
    "@angular/language-service": "8.2.0",
    "@ionic/angular-toolkit": "2.0.0",
    "@types/node": "12.6.9",
    "bread-compressor-cli": "1.0.7",
    "codelyzer": "5.1.0",
    "local-web-server": "3.0.7",
    "tslint": "5.18.0",
    "typescript": "3.4.5",
    "workbox-cli": "4.3.1"
  }
}
